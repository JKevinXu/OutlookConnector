{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
    "id": "95945cff-5a7d-490c-a979-d0b78f5257d7",
    "manifestVersion": "1.17",
    "version": "1.0.0",
    "name": {
        "short": "AM Personal Assistant",
        "full": "AM Personal Assistant - Seller Metrics & Email Analysis"
    },
    "description": {
        "short": "AI-powered personal assistant for Amazon sellers",
        "full": "AM Personal Assistant provides AI-powered email analysis and comprehensive seller metrics to help Amazon merchants optimize their business operations."
    },
    "developer": {
        "name": "AM Solutions",
        "websiteUrl": "https://www.amsolutions.com",
        "privacyUrl": "https://www.amsolutions.com/privacy",
        "termsOfUseUrl": "https://www.amsolutions.com/terms"
    },
    "icons": {
        "outline": "assets/outline.png",
        "color": "assets/color.png"
    },
    "accentColor": "#230201",
    "localizationInfo": {
        "defaultLanguageTag": "en-us",
        "additionalLanguages": []
    },
    "authorization": {
        "permissions": {
            "resourceSpecific": [
                {
                    "name": "MailboxItem.Read.User",
                    "type": "Delegated"
                }
            ]
        }
    },
    "validDomains": [
        "amsolutions.com",
        "jkevinxu.github.io"
    ],
    "extensions": [
        {
            "requirements": {
                "scopes": [
                    "mail"
                ],
                "capabilities": [
                    {
                        "name": "Mailbox",
                        "minVersion": "1.3"
                    }
                ]
            },
            "runtimes": [
                {
                    "requirements": {
                        "capabilities": [
                            {
                                "name": "Mailbox",
                                "minVersion": "1.3"
                            }
                        ]
                    },
                    "id": "TaskPaneRuntime",
                    "type": "general",
                    "code": {
                        "page": "{{BASE_URL}}taskpane.html"
                    },
                    "lifetime": "short",
                    "actions": [
                        {
                            "id": "TaskPaneRuntimeShow",
                            "type": "openPage",
                            "pinnable": false,
                            "view": "dashboard"
                        }
                    ]
                },
                {
                    "id": "CommandsRuntime",
                    "type": "general",
                    "code": {
                        "page": "{{BASE_URL}}commands.html",
                        "script": "{{BASE_URL}}commands.js"
                    },
                    "lifetime": "short",
                    "actions": [
                        {
                            "id": "action",
                            "type": "executeFunction"
                        }
                    ]
                }
            ],
            "ribbons": [
                {
                    "contexts": [
                        "mailRead"
                    ],
                    "tabs": [
                        {
                            "builtInTabId": "TabDefault",
                            "groups": [
                                {
                                    "id": "msgReadGroup",
                                    "label": "AM Personal Assistant",
                                    "icons": [
                                        {
                                            "size": 16,
                                            "url": "{{BASE_URL}}assets/icon-16.png"
                                        },
                                        {
                                            "size": 32,
                                            "url": "{{BASE_URL}}assets/icon-32.png"
                                        },
                                        {
                                            "size": 80,
                                            "url": "{{BASE_URL}}assets/icon-80.png"
                                        }
                                    ],
                                    "controls": [
                                        {
                                            "id": "msgReadOpenPaneButton",
                                            "type": "button",
                                            "label": "AM Personal Assistant",
                                            "icons": [
                                                {
                                                    "size": 16,
                                                    "url": "{{BASE_URL}}assets/icon-16.png"
                                                },
                                                {
                                                    "size": 32,
                                                    "url": "{{BASE_URL}}assets/icon-32.png"
                                                },
                                                {
                                                    "size": 80,
                                                    "url": "{{BASE_URL}}assets/icon-80.png"
                                                }
                                            ],
                                            "supertip": {
                                                "title": "AM Personal Assistant",
                                                "description": "AI-powered personal assistant for Amazon sellers with email analysis and seller metrics."
                                            },
                                            "actionId": "TaskPaneRuntimeShow"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
